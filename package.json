{
  "name": "tyatech-nestjs-system",
  "version": "3.0.3",
  "description": "",
  "author": "",
  "engines": {
    "node": "14.15.0"
  },
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist/*",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prestart": "rimraf dist",
    "start": "nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js --config src/configs/db/db.config.ts",
    "migration:generate": "npm run build && npm run typeorm migration:generate -- -n",
    "migration:run": "npm run build && npm run typeorm migration:run",
    "migration:revert": "npm run build && npm run typeorm migration:revert",
    "lib:remove-lib": "npx rimraf lib-npm",
    "lib:create-folder": "mkdir lib-npm && cd lib-npm && mkdir dist && cd dist && mkdir system-manager && cd ../../",
    "lib:copy-info": "copy .\\assets\\package.json .\\lib-npm\\ && copy .\\assets\\tyatech.cmd .\\lib-npm\\ && copy .\\README.md .\\lib-npm\\",
    "lib:copy-lib": "copy .\\dist\\main.d.ts .\\lib-npm\\dist\\ && copy .\\dist\\main.js .\\lib-npm\\dist\\ && copy .\\dist\\main.js.map .\\lib-npm\\dist\\ && Xcopy .\\dist\\system-manager .\\lib-npm\\dist\\system-manager /E /Y",
    "lib:build": "npm run prebuild && npm run build && echo Build Success && npm run lib:remove-lib && npm run lib:create-folder && npm run lib:copy-info && npm run lib:copy-lib",
    "lib:publish": "cd lib-npm && npm publish",
    "lib:deploy": "npm run lib:build && npm run lib:publish && echo Deploy Library Success && npm run lib:remove-lib"
  },
